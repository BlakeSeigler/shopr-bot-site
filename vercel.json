{
  "version": 2,
  "builds": [
    {
      "src": "Shopr/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "Shopr/build"
      }
    },
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}

  